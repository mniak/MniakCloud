variable "ProjectName" {
  description = "The project name"
  type        = string
  default     = "mniak-cloud"
}

variable "Region" {
  description = "The region to use"
  type        = string
  default     = "us-east1"
}

variable "Zone" {
  description = "The zone to use"
  type        = string
  default     = "us-east1-d"
}
